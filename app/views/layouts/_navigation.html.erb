  <% if !current_user  %>
   <ul class="nav pull-right">
     <li class="divider-vertical"><%= link_to "log in", log_in_path %></li>
     
     <li class="divider-vertical"><%= link_to "sign up", sign_up_path %></li>
   </ul>
 <% else %>
     <ul>
       <div class="btn-group pull-right">
         <a class="btn dropdown-@usertoggle" data-toggle="dropdown" href="#">
           <i class="icon-user"></i>
           <%= current_user.name %>
           <span class="caret"></span>
         </a>
         <ul class="dropdown-menu">
           <li><%= link_to 'profile', "#" %></li>
           <li class="divider"></li>
           <li><%= link_to 'logout', log_out_path, method: :delete, confirm: "Are you sure" %></li>
         </ul>
       </div>
     </ul>
  <% end %>
